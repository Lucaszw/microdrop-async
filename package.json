{
  "name": "@microdrop/async",
  "version": "0.2.18",
  "description": "Promise based library for controlling microdrop",
  "main": "MicrodropAsync.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build-dev": "npm run-script link-packages && webpack",
    "link-packages": "npm ln ../mqttclient-web",
    "update-deps": "ncu -ua @microdrop/async @microdrop/device-controller @microdrop/ui-plugin && npm install",
    "git-add": "git add package-lock.json && git add -p . && git commit",
    "publish-pkg": "npm run build && npm version patch && npm publish && git push origin master"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "dependencies": {
    "@mqttclient/node": "0.0.15",
    "@mqttclient/web": "0.1.5",
    "lodash": "^4.17.4",
    "mqtt": "^2.13.1"
  },
  "devDependencies": {
    "webpack": "^3.8.1"
  }
}
